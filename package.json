{
  "name": "ide-flowtype-coverage",
  "private": true,
  "version": "1.8.1",
  "description": "Atom IDE flowtype coverage support (temp solution)",
  "main": "lib/index.js",
  "scripts": {
    "test": "apm test",
    "lint": "eslint ."
  },
  "package-deps": [
    "linter"
  ],
  "engines": {
    "atom": ">=1.4.0 <2.0.0"
  },
  "providedServices": {
    "linter": {
      "versions": {
        "2.0.0": "provideLinter"
      }
    }
  },
  "consumedServices": {
    "status-bar": {
      "versions": {
        "^1.0.0": "consumeStatusBar"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guiguan/ide-flowtype-coverage.git"
  },
  "keywords": [
    "flow",
    "flowtype",
    "ide",
    "javascript"
  ],
  "contributors": [{
      "name": "Guan Gui"
    },
    {
      "name": "steelbrain"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/guiguan/ide-flowtype-coverage/issues"
  },
  "homepage": "https://github.com/guiguan/ide-flowtype-coverage#readme",
  "dependencies": {
    "atom-autocomplete": "^1.0.0",
    "atom-package-deps": "^4.4.1",
    "sb-string_score": "^0.1.20",
    "atom-linter": "^10.0.0"
  },
  "devDependencies": {
    "eslint-config-steelbrain": "^3.0.1",
    "eslint-plugin-flowtype": "^2.33.0",
    "flow-bin": "^0.52.0"
  },
  "configSchema": {
    "onlyIfAppropriate": {
      "title": "Only activate when .flowconfig exists",
      "type": "boolean",
      "default": true
    },
    "showUncovered": {
      "title": "Show uncovered code in the editor",
      "type": "boolean",
      "default": false
    },
    "executablePath": {
      "description": "Path to `flow` executable",
      "type": "string",
      "default": ""
    },
    "hyperclickPriority": {
      "description": "Priority to use for hyperclick provider (requires restart)",
      "type": "integer",
      "default": "0"
    }
  }
}
